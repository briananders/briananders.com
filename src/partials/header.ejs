<%- partial('nav', locals); %>
<header class="main-header <%- locals.withHead ? 'with-head' : 'without-head' %>">
  <h1 itemprop="title" class="name content">
    <% if (locals.altTitle !== undefined) { %>
      <%- noWidows(locals.altTitle); %>
    <% } else if (locals.title !== undefined) { %>
      <%- noWidows(locals.title); %>
    <% } %>
  </h1>
</header>
