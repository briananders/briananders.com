{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jquery": true
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "after": false,
    "afterEach": false
  },
  "rules": {
    "func-names": 0,
    "no-var": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0,
    "import/newline-after-import": 0,
    "lines-around-directive": 0,
    "arror-parens": 0,
    "no-prototype-builtins": 0,
    "no-plusplus": 0,
    "global-require": 1,
    "strict": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": [2, {
      "objects": "always-multiline",
      "function": "ignore"
    }],
    "no-confusing-arrow": ["warn", {
      "allowParens": true
    }],
  }
}